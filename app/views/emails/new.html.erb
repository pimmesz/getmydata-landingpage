<h1>GetMyData</h1>
<p>Find me in app/views/emails/new.html.erb</p>

<%= form_for(@email) do |f| %>
  <div class="form-group">
    <%= f.label :name %>
    <%= f.text_field :name, class: 'form-control' %>
  </div>

  <div class="form-group">
    <%= f.label :email %>
    <%= f.text_field :email, class: 'form-control' %>
  </div>

  <%= f.submit "Submit", class: 'btn btn-primary' %>
<% end %>
